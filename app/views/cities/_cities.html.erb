<div>
  <div class="m-3 text-xl font-bold text-gray-900 ">
    <ul  style="list-style-type:disc;" >
      <li >
        <div class="flex flex-col justify-center">
            <%=link_to "#{city.name}" , admin_office_path(:city => city.id ) %>
          <span class="w-1/3 mt-3  mr-6 p-0.5 bg-gray-800 "></span>
          <div class="flex mt-3 text-lg font-medium text-gray-700">
            <%if city.offices.length > 0 %>
              <%city.offices.each do |office| %>
                <p><%=office.name%><p class="flex flex-col justify-end text-sm  text-gray-600">[35]workflows</p> ,</p>
              <%end%>
            <%else%>
              <p>Theres is not any offices registered yet</p>
            <%end %>
          </div>
        </div>
      </li>
    </ul>
  </div>
</div>