

<% if user_signed_in? %>
  <%= current_user.email %>
  

<%end %>

<div class="flex justify-center w-auto">
  <% flash.each do |type, msg| %>
    <div class="bg-red-100 border border-red-400 text-red-700 px-4 py-3 rounded relative" role="alert">
      <strong class="font-bold"><%=type %>!</strong>
      <span class="block sm:inline"><%=msg %></span>

    </div>
  <% end %>
</div>




<div class="flex flex-col items-center pt-10  px-20">
     <div class="flex justify-center w-full">
       <button type="button" class="text-gray-500 border-2 border-indigo-800 font-medium  text-sm px-5 py-2.5 text-center " disabled>User</button>
       <button type="button" class="text-white bg-indigo-800 font-medium  text-sm px-5 py-2.5 text-center " disabled>Admin</button>
     </div>
</div>