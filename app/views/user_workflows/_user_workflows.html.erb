<div>
  <div class="m-3 text-xl font-bold text-gray-900 ">
    <ul  style="list-style-type:disc;" >
      <li >
        <div class="flex flex-col justify-center">
          <%=link_to "#{workflow.workflow_name}", admin_edit_user_workflow_path(workflow_id: workflow.id)%>
          <span class="w-1/3 mt-3  mr-6 p-0.5 bg-gray-800 "></span>
          <div class="flex mt-3 text-lg font-medium text-gray-700">

            <%if workflow.users.length > 0 %>
            <% workflow.users.each do |user| %>
                            <p class=""><%=user.user_profile.nmae%>,</p>
            <%end%>
            <%else%>
                          <p>Edit</p>
            <%end %>

          </div>
        </div>
      </li>
    </ul>
  </div>
</div>