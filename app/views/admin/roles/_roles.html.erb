<div>
  <div class="m-3 text-xl font-bold text-gray-900 ">
    <ul  style="list-style-type:disc;" >
      <li >
        <div class="flex flex-col justify-center">
          <%#=link_to "#{office.name}" , admin_user_workflow_path(:office_id => office.id)%>
          <%=role.name %>
          <span class="w-1/3 mt-3  mr-6 p-0.5 bg-gray-800 "></span>
          <div class="flex mt-3 text-sm font-medium text-gray-700">
            <% if role.permissions.length > 0%>
              <% role.permissions.each do |permission| %>
              <p><%=permission.name %>,</p>
              <%end %>
            <% elsif role.name == "admin"%>
              <p>Have all access to the system</p>
            <%end %>
          </div>
        </div>
      </li>
    </ul>
  </div>
</div>